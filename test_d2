#! /usr/bin/octave -qf
format long g
disp("");

	##   M      измеренное значение размера по роликам, мм
	##   d      номинальное значение наружнго диаметра резьбы, мм
	##   P      номинальное значение шага резьбы, мм
	##   Ph     номинальное значение хода резьбы, мм
	##   ang    угол профиля резьбы, град
	##   dalp   отклонение угла профиля, мин
	##   ddpref отклонения диаметров проволочек (3-вектор), мкм
	##   C5     поправка, учитывающая измерительное усилие 2Н, мкм

disp("5\"-3 ACME-2 STARTS-3G-LEFT\n");
M = 131.907;
d = 122.81;
P = 25.4/3;  #8.4667
Ph = P*2;    #16.933
ang = 29;
dalp = 0;
ddpref = [0 0 0];
C5 = 0.4;
d2 = roll3(M, d, P, Ph, ang, dalp, ddpref, C5);
printf("PlugGo d2: %.3f\n\n", d2);


disp("\n===============\n");


disp("Tr130x24(P12)-7e\n");
M = 131.907;
d = 130;
P = 12;
Ph = 24;
ang = 30;
dalp = 0;
ddpref = [0 0 0];
C5 = 0.4;
d2 = roll3(M, d, P, Ph, ang, dalp, ddpref, C5);
printf("PlugGo d2: %.3f\n\n", d2);

M = 132.551;
d2 = roll3(M, d, P, Ph, ang, dalp, ddpref, C5);
printf("PlugNG d2: %.3f\n\n", d2);

M = 131.726;
d2 = roll3(M, d, P, Ph, ang, dalp, ddpref, C5);
printf("CPlugGo d2: %.3f\n\n", d2);

M = 131.214;
d2 = roll3(M, d, P, Ph, ang, dalp, ddpref, C5);
printf("CPlugNG d2: %.3f\n\n", d2);

